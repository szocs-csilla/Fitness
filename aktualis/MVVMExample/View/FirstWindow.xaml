<Window x:Class="View.FirstWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Left="100" 
             Top="50"
             MinWidth="900" MinHeight="550"
             MaxWidth="900" MaxHeight="550"
             Background="DarkGray">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="150">

        <Button Content="User" Margin="10" Command="{Binding UserCommand}"></Button>
        <Button Content="Admin" Margin="10" Command="{Binding LoginCommand}"></Button>

    </StackPanel>
    
</Window>
